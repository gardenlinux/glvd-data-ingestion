    "version": "0.2.0",
    "configurations": [
        {
            "name": "debsec_cve",
            "type": "debugpy",
            "request": "launch",
            "module": "glvd.data.debsec_cve",
            "cwd": "${workspaceFolder}/src",
            "args": ["${workspaceFolder}/working-data/debian-security-tracker.json"]
        },
        {
            "name": "debsrc",
            "type": "debugpy",
            "request": "launch",
            "module": "glvd.data.debsrc",
            "cwd": "${workspaceFolder}/src",
            "args": ["${workspaceFolder}/working-data/package-list"]
        },
        {
            "name": "combine_deb",
            "type": "debugpy",
            "request": "launch",
            "module": "glvd.cli.data.combine_deb",
            "cwd": "${workspaceFolder}/src",
            "args": ["--database=postgresql+asyncpg://glvd:glvd@localhost:5432/glvd"]
        }
    ]
}
